{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": false,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@app/*": [
        "./app/*"
      ],
      "@src/*": [
        "./src/*"
      ],
      "@features/*": [
        "./src/features/*"
      ],
      "@assets/*": [
        "./assets/*"
      ],
      "@sharedcomponents/*": [
        "./src/shared/components/*"
      ],
      "@sharedvalidation/*": [
        "./src/shared/validation/*"
      ],
      "@reports/*": [
        "./src/features/reports/*"
      ]
    }
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "app/**/*.ts",
    "app/**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
  "exclude": [
    "node_modules",
    "**/*.spec.ts",
    "**/*.test.ts"
  ]
}
